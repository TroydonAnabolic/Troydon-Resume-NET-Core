@model IEnumerable<Troydon_Resume_Online_NET_Core_.Models.Comment>
@{
    ViewData["Title"] = "Comments";
}

    <h1>Feedback</h1>

<p>
    Below are the latest comments from our reviewers
</p>

<div class="content-wrap">
    @foreach (var comment in Model)
    {
        @await Html.PartialAsync("Comment", comment)
    }
</div>